<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.carry.pojo">
	<class name="Stu" table="stuinfo">
		<id name="id" column="id">
		</id>
		<property name="name" column="name"></property>
		<property name="gender" column="gender"></property>
		<property name="enteryear" column="enteryear"></property>
		<property name="nativeplace" column="nativeplace"></property>
		<property name="phone" column="phone"></property>
		<property name="major" column="major"></property>
	</class>
</hibernate-mapping>